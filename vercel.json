{
  "crons": [
    {
      "path": "/api/worker",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/invoice-worker",
      "schedule": "* * * * *"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "includeFiles": "src/**"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
