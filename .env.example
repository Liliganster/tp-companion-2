# Client (Vite) - these are bundled into the browser when prefixed with VITE_
VITE_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Maps (client key must be public; restrict by HTTP referrer + only "Maps JavaScript API")
VITE_GOOGLE_MAPS_BROWSER_KEY=your_google_maps_browser_key

# Server-only (do NOT prefix with VITE_)
# Used by serverless endpoints (Calendar/Drive) and Google Maps proxies (Directions/Geocoding/Places)
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

GOOGLE_MAPS_SERVER_KEY=your_google_maps_server_key

# Google OAuth (Calendar/Drive)
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id
GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:8080/api/google/oauth/callback
GOOGLE_OAUTH_STATE_SECRET=your_random_state_signing_secret

# AI & Extraction (Universal Extractor)
GEMINI_API_KEY=your_gemini_api_key
CRON_SECRET=optional_cron_secret_for_vercel
