# Client (Vite) - these are bundled into the browser when prefixed with VITE_
VITE_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Maps (client key must be public; restrict by HTTP referrer + only "Maps JavaScript API")
VITE_GOOGLE_MAPS_BROWSER_KEY=your_google_maps_browser_key

# Google Drive Picker (client API key; restrict by HTTP referrer + enable "Google Picker API" + "Google Drive API")
VITE_GOOGLE_PICKER_API_KEY=your_google_picker_api_key

# Server-only (do NOT prefix with VITE_)
# Used by serverless endpoints (Calendar/Drive) and Google Maps proxies (Directions/Geocoding/Places)
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

GOOGLE_MAPS_SERVER_KEY=your_google_maps_server_key

# Google OAuth (Calendar/Drive)
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id
GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:8080/api/google/oauth/callback
GOOGLE_OAUTH_STATE_SECRET=your_random_state_signing_secret

# Electricity Maps (server-only)
ELECTRICITY_MAPS_API_KEY=your_electricity_maps_api_key
ELECTRICITY_MAPS_DEFAULT_ZONE=AT

# Climatiq (server-only)
CLIMATIQ_API_KEY=your_climatiq_api_key
# Optional overrides (advanced)
# CLIMATIQ_DATA_VERSION=^21
# CLIMATIQ_ACTIVITY_ID_GASOLINE=fuel-type_motor_gasoline-fuel_use_na
# CLIMATIQ_ACTIVITY_ID_DIESEL=fuel-type_diesel-fuel_use_na

# AI & Extraction (Universal Extractor)
GEMINI_API_KEY=your_gemini_api_key
CRON_SECRET=optional_cron_secret_for_vercel

# Observability (optional)
# Client (Vite)
VITE_SENTRY_DSN=
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
VITE_SENTRY_REPLAY_SESSION_SAMPLE_RATE=0
VITE_SENTRY_REPLAY_ERROR_SAMPLE_RATE=0
VITE_GA_MEASUREMENT_ID=

# Server (Vercel functions)
SENTRY_DSN=
SENTRY_ENVIRONMENT=
SENTRY_TRACES_SAMPLE_RATE=0.05
LOG_LEVEL=info

# Rate limiting (optional, recommended for production)
# Upstash Redis REST credentials:
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Plan limits (optional)
# For testing/staging: bypass monthly AI extraction quota checks (still rate-limited for abuse).
BYPASS_AI_LIMITS=0
